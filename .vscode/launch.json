            "name": "Spring Boot-ShophubbackendApplication",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.example.shophubbackend.ShophubbackendApplication",
            "projectName": "shophubbackend",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "vmArgs": "-Dspring.profiles.active=dev"
