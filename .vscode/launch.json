            "vmArgs": "-Dspring.profiles.active=dev -Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=5005"
        },
        {
            "type": "java",
            "name": "Run Tests",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "",
